{
	"name": "util-http",
	"version": "0.0.12",
	"description": "util-http",
	"author": "Tutitoos",
	"license": "MIT",
	"repository": "Tutitoos/util-http.git",
	"publisher": "Tutitoos",
	"keywords": [
		"http",
		"axios",
		"fetch",
		"undici",
		"http-client",
		"http-requests",
		"typescript",
		"nodejs",
		"utilities",
		"rest-api"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"postbuild": "cp src/types.d.ts dist/types.d.ts",
		"start": "node dist/index.js",
		"dev": "ts-node-dev --respawn --transpile-only --poll src/index.ts",
		"lint": "pnpm run lint:eslint && pnpm run lint:biome",
		"lint:eslint": "npx eslint --ignore-path .gitignore . --max-warnings 0",
		"lint:biome": "npx @biomejs/biome check src",
		"pub": "npm publish",
		"adduser": "npm adduser"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/node": "24.6.2",
		"@typescript-eslint/eslint-plugin": "8.45.0",
		"@typescript-eslint/parser": "8.45.0",
		"eslint": "9.37.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-config-xo": "0.49.0",
		"eslint-config-xo-typescript": "9.0.0",
		"eslint-import-resolver-alias": "1.1.2",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import": "2.32.0",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-promise": "7.2.1",
		"ts-node-dev": "2.0.0",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"axios": "1.12.2",
		"undici": "7.16.0"
	}
}
